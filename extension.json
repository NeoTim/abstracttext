{
  "name": "AbstractText",
  "version": "0.1.0",
  "author": [
    "Denny Vrandecic"
  ],
  "url": "https://github.com/google/abstracttext",
  "descriptionmsg": "abstracttext-desc",
  "type": "other",
  "license-name": "Apache-2.0",
  "APIModules": {
    "abstracttext": "AbstractTextAPI"
  },
  "AutoloadClasses": {
    "AbstractText\\AbstractTextContent": "includes/AbstractTextContent.php",
    "AbstractText\\AbstractTextContentHandler": "includes/AbstractTextContentHandler.php",
    "AbstractText\\AbstractTextHooks": "includes/AbstractTextHooks.php",
    "AbstractText\\Helper": "includes/Helper.php",
    "AbstractTextAPI": "includes/AbstractTextAPI.php"
  },
  "MessagesDirs": {
    "AbstractText": [
      "i18n"
    ]
  },
  "namespaces": [
    {
      "id": 78,
      "constant": "NS_MEANING",
      "name": "M",
      "capitallinkoverride": false,
